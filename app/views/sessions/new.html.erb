<h1>Sign In</h1>
<%= form_with url: sign_in_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email, placeholder: "john@doe.com" %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= link_to "Forgot password", password_reset_path %>
  </div>
  <%= form.submit "Sign In" %>
<% end %>
